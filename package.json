{
  "private": true,
  "workspaces": [
    "src/"
  ],
  "scripts": {
    "// Demo using vite-plugin-ssr": "",
    "demo": "cd examples/vue/ && npm run start",
    "// Build vite-plugin-ssr": "",
    "build": "npm install && npm run tsc:once",
    "// Develop vite-plugin-ssr": "",
    "dev": "npm run tsc:watch",
    "// TypeScript": "",
    "tsc:once": "cd src/ && tsc",
    "tsc:watch": "cd src/ && tsc --incremental --watch --preserveWatchOutput",
    "// Release new version": "",
    "release": "npm run clean:all && npm install && npm run tsc:once && npm run publish",
    "// Clean": "",
    "clean": "npm run clean:all",
    "clean:all": "git clean -Xdf"
  },
  "devDependencies": {
    "@types/es-module-lexer": "^0.3.0",
    "es-module-lexer": "^0.3.26"
  }
}
