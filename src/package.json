{
  "name": "vite-plugin-ssr",
  "version": "0.1.2",
  "dependencies": {
    "@brillout/json-s": "^0.2.0",
    "@brillout/libassert": "^0.4.0",
    "@brillout/path-to-regexp": "^0.1.2",
    "cac": "^6.7.3",
    "devalue": "^2.0.1",
    "fast-glob": "^3.2.5",
    "kolorist": "^1.4.1"
  },
  "devDependencies": {
    "@types/node": "^15.6.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.2.4"
  },
  "peerDependencies": {
    "vite": "^2.2.3"
  },
  "engines": {
