{
  "scripts": {
    "start": "npm run dev",
    "dev": "ts-node ./server",
    "prod": "npm run build && npm run prod:serve",
    "// Build for production": "",
    "build": "npm run build:clean && npm run build:vite && npm run build:prerender",
    "build:clean": "rimraf dist/",
    "build:vite": "vite build && vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender",
    "// Static serve for production": "",
    "prod:serve": "serve dist/client/ --listen 3000"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/express": "^4.17.13",
    "@types/node": "^16.11.11",
    "@types/node-fetch": "^2.5.12",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.0",
    "cross-fetch": "^3.1.4",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rimraf": "^3.0.2",
    "serve": "^13.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "vite": "^2.6.14",
    "vite-plugin-mdx": "3.5.10",
    "vite-plugin-ssr": "0.3.34"
  },
