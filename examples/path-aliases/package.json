{
  "imports": {
    "#root/*": "./*.ts"
  },
  "scripts": {
    "dev": "npm run server:dev",
    "prod": "npm run build && npm run server:prod",
    "prod:static": "npm run build && npm run build:prerender && npm run server:static",
    "build": "vite build && vite build --ssr",
    "build:prerender": "vite-plugin-ssr prerender",
    "server:dev": "ts-node ./server",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server",
    "server:static": "serve dist/client/ --listen 3000"
  },
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.8",
    "@types/react": "^17.0.38",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.1.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "serve": "^13.0.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4",
    "vite": "^2.7.12",
    "vite-plugin-ssr": "0.3.51"
  },
  "// Needed for Yarn workspaces": "",
  "name": "example-path-aliases",
  "version": "0.0.0"
}
