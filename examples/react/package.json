{
  "scripts": {
    "// User scripts": "",
    "start": "npm install && npm run server",
    "dev": "npm run server:watch",
    "prod": "npm run build && npm run server:prod",
    "// Build app": "",
    "build": "vite build && vite build --ssr",
    "// Start server": "",
    "server": "ts-node ./server",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server",
    "server:watch": "ts-node-dev ./server"
  },
  "dependencies": {
    "@brillout/vite-plugin-mdx": "0.1.4",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@types/express": "^4.17.11",
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.2",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "node-fetch": "^3.0.0-beta.9",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5",
    "vite": "2.0.1",
    "vite-plugin-ssr": "^0.1.0-beta.9"
  }
}
