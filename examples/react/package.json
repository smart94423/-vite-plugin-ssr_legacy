{
  "scripts": {
    "start": "npm install && npm run vite-plugin-ssr:build && npm run server:run",
    "dev": "concurrently \"npm run server:watch\" \"npm run vite-plugin-ssr:watch\" --kill-others",
    "server:run": "ts-node ./server.ts",
    "server:watch": "ts-node-dev ./server.ts",
    "vite-plugin-ssr:build": "cd ../../ && npm run build",
    "vite-plugin-ssr:watch": "cd ../../ && npm run dev"
  },
  "dependencies": {
    "react": "^17.0.1",
    "react-dom": "^17.0.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.2",
    "concurrently": "^5.3.0",
    "express": "^4.17.1",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.60"
  }
}
