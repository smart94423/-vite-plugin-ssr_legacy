    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-react-refresh": "^1.1.2",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "vite": "^2.0.0-beta.50"
  },
  "type": "module"
}
