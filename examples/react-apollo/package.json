    "cross-fetch": "^3.1.2",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "serve": "^11.3.2",
    "vite": "2.1.2",
    "vite-plugin-ssr": "0.1.0-beta.17"
  }
}
