{
  "scripts": {
    "// User scripts": "",
    "start": "npm run vite-plugin-ssr:build && npm install && npm run server",
    "dev": "concurrently \"npm run server:watch\" \"npm run vite-plugin-ssr:watch\" --kill-others",
    "prod": "npm run build && npm run server:prod",
    "// Build app": "",
    "build": "vite build && vite build --ssr",
    "// Start server": "",
    "server": "ts-node ./server",
    "server:prod": "cross-env NODE_ENV=production ts-node ./server",
    "server:watch": "ts-node-dev ./server",
    "// Build/watch vite-plugin-ssr": "",
    "vite-plugin-ssr:build": "cd ../../ && npm run build",
    "vite-plugin-ssr:watch": "cd ../../ && npm run dev"
  },
  "dependencies": {
    "@types/node": "^14.14.22",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "@vitejs/plugin-vue": "^1.0.0",
    "@vitejs/plugin-vue-jsx": "^1.1.0",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/server-renderer": "^3.0.5",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "node-fetch": "^3.0.0-beta.9",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.1",
    "typescript": "^4.1.5",
    "vite": "2.0.1",
    "vite-plugin-ssr": "file:../../src/",
    "vue": "^3.0.5"
  }
}
